{
  "name": "backend",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "services/*",
    "packages/*"
  ],
  "main": "server.js",
  "scripts": {
    "test": "jest --config=jest.config.js",
    "test:coverage": "jest --config=jest.config.js --coverage",
    "test:watch": "jest --config=jest.config.js --watch",
    "test:ci": "jest --config=jest.config.js --ci --coverage",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "start": "node dist/server.js",
    "dev": "nodemon --exec ts-node server.ts",
    "start:gateway": "node dist/api-gateway/src/index.js",
    "start:auth": "node dist/services/auth-service/src/index.js",
    "start:user": "node dist/services/user-service/src/index.js",
    "start:content": "node dist/services/content-service/src/index.js",
    "start:monitoring": "node dist/services/monitoring-service/src/index.js",
    "start:notification": "node dist/services/notification-service/src/index.js",
    "start:subscription": "node dist/services/subscription-management-service/src/index.js",
    "dev:gateway": "nodemon --exec ts-node api-gateway/src/index.ts",
    "dev:auth": "nodemon --exec ts-node services/auth-service/src/index.ts",
    "dev:user": "nodemon --exec ts-node services/user-service/src/index.ts",
    "dev:content": "nodemon --exec ts-node services/content-service/src/index.ts",
    "dev:monitoring": "nodemon --exec ts-node services/monitoring-service/src/index.ts",
    "dev:notification": "nodemon --exec ts-node services/notification-service/src/index.ts",
    "dev:subscription": "nodemon --exec ts-node services/subscription-management-service/src/index.ts",
    "dev:all": "concurrently \"npm run dev:gateway\" \"npm run dev:auth\" \"npm run dev:user\" \"npm run dev:content\" \"npm run dev:monitoring\" \"npm run dev:notification\"",
    "lint": "eslint . --ext .ts,.js",
    "lint:fix": "eslint . --ext .ts,.js --fix",
    "db:seed": "ts-node scripts/seed.ts",
    "db:migrate": "ts-node scripts/migrate.ts",
    "db:pg:migrate": "ts-node scripts/pg-migrate.ts",
    "db:pg:seed": "ts-node scripts/pg-seed.ts",
    "db:es:init": "ts-node scripts/es-init.ts",
    "db:es:reindex": "ts-node scripts/es-reindex.ts",
    "db:init": "ts-node src/scripts/init-databases.ts",
    "db:sync": "ts-node src/scripts/sync-databases.ts",
    "db:monitor": "ts-node src/scripts/monitor-databases.ts",
    "db:migrate:generate": "ts-node src/scripts/manage-migrations.ts generate pg",
    "db:migrate:run": "ts-node src/scripts/manage-migrations.ts run",
    "db:migrate:revert": "ts-node src/scripts/manage-migrations.ts revert pg",
    "db:migrate:list": "ts-node src/scripts/manage-migrations.ts list pg",
    "db:es:indices": "ts-node src/scripts/manage-migrations.ts create-es-indices",
    "db:mongo:schema": "ts-node src/scripts/manage-migrations.ts update-mongo-schema",
    "security:audit": "npm audit --production",
    "analyze": "ts-node scripts/analyze-bundle.ts",
    "docs:generate": "ts-node scripts/generate-docs.ts",
    "type-check": "tsc --noEmit",
    
    "containerization:validate:module1": "bash scripts/validate-containerization.sh 1",
    "containerization:validate:module2": "bash scripts/validate-containerization.sh 2",
    "containerization:validate:module3": "bash scripts/validate-containerization.sh 3",
    "containerization:validate:module4": "bash scripts/validate-containerization.sh 4",
    "containerization:validate:module5": "bash scripts/validate-containerization.sh 5",
    "containerization:validate:module6": "bash scripts/validate-containerization.sh 6",
    "containerization:validate:all": "bash scripts/validate-containerization.sh all",
    
    "health:check:all": "concurrently \"npm run health:check:gateway\" \"npm run health:check:auth\" \"npm run health:check:user\" \"npm run health:check:content\"",
    "health:check:gateway": "curl -f http://localhost:5001/health || exit 1",
    "health:check:auth": "curl -f http://localhost:3001/health || exit 1",
    "health:check:user": "curl -f http://localhost:3002/health || exit 1",
    "health:check:content": "curl -f http://localhost:3003/health || exit 1",
    
    "test:services:health": "npm run health:check:all",
    "test:inter-service-communication": "jest --testPathPattern=integration --testNamePattern=\"inter-service\"",
    "test:database-connections": "jest --testPathPattern=database --testNamePattern=\"connection\"",
    
    "performance:test": "concurrently \"npm run performance:test:memory\" \"npm run performance:test:cpu\"",
    "performance:test:memory": "node scripts/performance/memory-test.js",
    "performance:test:cpu": "node scripts/performance/cpu-test.js",
    
    "security:scan": "npm run security:scan:trivy && npm run security:scan:snyk",
    "security:scan:trivy": "trivy fs --severity HIGH,CRITICAL .",
    "security:scan:snyk": "snyk test --severity-threshold=high",
    "security:test:secrets": "node scripts/security/secret-scanner.js",
    "security:test:network": "node scripts/security/network-test.js",
    
    "monitoring:test:alerts": "node scripts/monitoring/alert-test.js",
    "monitoring:verify": "npm run monitoring:test:alerts && curl -f http://localhost:9090/targets"
  },
  "keywords": [
    "microservices",
    "api",
    "backend",
    "express",
    "mongodb"
  ],
  "author": "Corp Astro Team",
  "license": "ISC",
  "description": "Backend services for SAP project",
  "dependencies": {
    "@corp-astro/shared-types": "file:../shared/types",
    "@supabase/supabase-js": "^2.49.8",
    "amqplib": "^0.10.3",
    "apollo-server-express": "^3.13.0",
    "axios": "^1.10.0",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^2.2.0",
    "compression": "^1.8.0",
    "concurrently": "^8.2.2",
    "connect-redis": "^7.1.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "detect-port": "^2.1.0",
    "dotenv": "^16.4.5",
    "elasticsearch": "^16.7.3",
    "express": "^5.1.0",
    "express-rate-limit": "^7.2.0",
    "express-session": "^1.18.0",
    "express-validator": "^7.0.1",
    "graphql": "^16.8.1",
    "helmet": "^7.1.0",
    "hpp": "^0.2.3",
    "http-proxy-middleware": "^2.0.6",
    "ioredis": "^5.3.2",
    "jest": "^29.7.0",
    "joi": "^17.12.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose-delete": "^1.0.1",
    "mongoose-encryption": "^2.1.2",
    "mongoose-history": "^0.8.0",
    "mongoose-paginate-v2": "^1.8.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.10.1",
    "otplib": "^12.0.1",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pdfkit": "^0.14.0",
    "pg": "^8.11.3",
    "qrcode": "^1.5.4",
    "redis": "^4.6.13",
    "reflect-metadata": "^0.2.1",
    "slugify": "^1.6.6",
    "socket.io": "^4.7.5",
    "speakeasy": "^2.0.0",
    "supertest": "^6.3.4",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "tsconfig-paths": "^4.2.0",
    "typeorm": "^0.3.20",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "winston-elasticsearch": "^0.11.0",
    "winston-mongodb": "^5.1.1",
    "xss-clean": "^0.1.4"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@types/accepts": "^1.3.7",
    "@types/bcrypt": "^5.0.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.8.1",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.19",
    "@types/elasticsearch": "^5.0.43",
    "@types/express": "^5.0.1",
    "@types/express-serve-static-core": "^5.0.7",
    "@types/express-validator": "^2.20.33",
    "@types/hpp": "^0.2.6",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/long": "^4.0.2",
    "@types/methods": "^1.1.4",
    "@types/mongoose": "^5.11.96",
    "@types/morgan": "^1.9.10",
    "@types/node": "^22.15.12",
    "@types/nodemailer": "^6.4.17",
    "@types/passport": "^1.0.17",
    "@types/passport-google-oauth20": "^2.0.16",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/pg": "^8.15.4",
    "@types/phoenix": "^1.6.6",
    "@types/qrcode": "^1.5.5",
